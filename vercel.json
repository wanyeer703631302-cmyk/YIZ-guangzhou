{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "app/dist",
  "installCommand": "npm install --prefix . --legacy-peer-deps && npm install --prefix app",
  "framework": null,
  "rewrites": [
    {
      "source": "/((?!api).*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/**/*.ts": {
      "runtime": "@vercel/node@3.0.0",
      "maxDuration": 10
    }
  },
  "env": {}
}
